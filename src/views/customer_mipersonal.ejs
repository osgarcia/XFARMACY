<!DOCTYPE html>
<html lang="en">

    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="description" content="">
        <meta name="author" content="">

        <link href="/assets/img/favico.ico" rel="icon">

        <title>Portal-MED</title>

        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" integrity="sha512-iecdLmaskl7CVkqkXNQ/ZH/XLlvWZOJyj7Yy7tcenmpD1ypASozpmT/E0iPtmFIB46ZmdtAc9eNBvH0H/ZpiBw==" crossorigin="anonymous" referrerpolicy="no-referrer" />

        <!-- Bootstrap Core CSS -->
        <link href="/assets/css/bootstrap.min.css" rel="stylesheet">

        <!-- MetisMenu CSS -->
        <link href="/assets/css/metisMenu.min.css" rel="stylesheet">

        <!-- Timeline CSS -->
        <link href="/assets/css/timeline.css" rel="stylesheet">

        <!-- Custom CSS -->
        <link href="/assets/css/startmin.css" rel="stylesheet">



        <!-- Custom Fonts -->
        <link href="/assets/css/font-awesome.min.css" rel="stylesheet" type="text/css">

        
    </head>

    <body>

        <form action="/mipersonal/<%= Nuser %>" method="get">
        <div id="portal">
            <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
                <div class="navbar-header">
                    <a class="navbar-brand" href="#">Portal-MED</a>
                </div>
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

                 <ul class="nav navbar-right navbar-top-links">
                    <li class="dropdown">
                        <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                            <i class="fa fa-user fa-fw"></i> <%= Nuser %> <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu dropdown-user">
                            <li>
                                <a href="#"><i class="fa fa-user fa-fw"></i> Perfil De Usuario</a>
                            </li>
                            <li>
                                <a href="#"><i class="fa fa-gear fa-fw"></i> Configuracion</a>
                            </li>
                            <li class="divider"></li>
                            <li>
                                <a href="/"><i class="fa fa-sign-out fa-fw"></i> Salir</a>
                            </li>
                        </ul>
                    </li>
                </ul>
            </nav>

            <aside class="sidebar navbar-default" role="navigation">
                <div class="sidebar-nav navbar-collapse">
                    <ul class="nav" id="side-menu">
                        <li class="sidebar-search">
                            <div class="input-group custom-search-form">
                                <input type="text" class="form-control" placeholder="Search...">
                                <span class="input-group-btn">
                                    <button class="btn btn-primary" type="button">
                                        <i class="fa fa-search"></i>
                                    </button>
                                </span>
                            </div>
                        </li>
                        <li>
                            <a href="/portalmed/<%= User %>" class="active"><i class="fa fa-home fa-2x"></i> Inicio</a>
                        </li>
                        <li>
                            <a href="/portalmedjf/<%= User %>" class="active"><i class="fa fa-id-card fa-2x"></i> Solicitudes</a>
                        </li>
                        <li>
                            <a href="/mipersonal/<%= User %>" class="active"><i class="fa fa-users fa-2x"></i> Mi Personal</a>
                        </li>
                      
            </aside>

            <div id="page-portal">
                <h1 class="page-header">Mi Personal</h1>
                <div class="container mt-5" >
                    <div class="row d-flex justify-content-center align-items-center h-100">
                    <div class="col-md-5">
                        <div class="card ">
                            <div class="card-body p-5 shadow-5 text-center">
                                <b><label class="formkit-label" for="input_0"><h5></h5></label></b>          
                                <p>                     
                                </p>
                                <div class="container mt-5">
                                    <div class="row">
                                        <div class="col-md-15">
                                    
                                      <b><label class="formkit-label" for="input_0"><h5><b></b></h5></label></b>
                                      <p>
                                         <table id = "pacientes" class="table table-bordered table-hover text-center">
                                             <thead>
                                                 <tr class="table-dark">
                                         
                                                     <th class="text-center">
                                                         No. Empleado
                                                     </th>
                                                     <th class="text-center">
                                                         Colaborador
                                                   </th>
                                                     <th class="text-center">
                                                        Fecha De Ingreso
                                                     </th>
                                                     <th class="text-center">
                                                        Puesto
                                                     </th>
                                                     <th class="text-center">
                                                        Vacaciones Disponibles
                                                     </th>
                        
                                             </thead>
                                 </p>
                          
                                 <tbody> 
                                     <% if (data) { %>
                                         <% for (var i = 0; i < data.length; i++) { %>
                                         <tr>
                                     
                                            
                                             <td><%= data[i].ID %></td>
                                             <td><%= data[i].Nuser %></td>
                                             <td><%= data[i].Fecingre %></td>
                                             <td><%= data[i].Puesto %></td>
                                             <td><%= data[i].Dvacs %></td>
                                             
                                         </tr>
                                         <% } %>
                                         <% } %>
                                             </tbody>
                                            </table>
                                          </div>
                                        </div>
                                      </div>
                                </div>
                              </div>
                               
                            </div>
                            </div>
                        </div>
                    </div>
                    </div>
                </div>
            </div>
        </div>               
    </form>
        <!-- jQuery -->
        <script src="/assets/js/jquery.min.js"></script>

        <!-- Bootstrap Core JavaScript -->
        <script src="/assets/js/bootstrap.min.js"></script>

        <!-- Metis Menu Plugin JavaScript -->
        <script src="/assets/js/metisMenu.min.js"></script>

        <!-- Morris Charts JavaScript -->
        <script src="/assets/js/raphael.min.js"></script>
      
        <!-- Custom Theme JavaScript -->
        <script src="/assets/js/startmin.js"></script>

        <script src="https://code.jquery.com/jquery-3.7.0.js"></script>

    </body>

</html>