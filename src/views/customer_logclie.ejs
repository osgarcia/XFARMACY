<!-- Section: Design Block -->
<section class="text-center text-lg-start">
    <style>
      .cascading-right {
        margin-right: -50px;
      }
  
      @media (max-width: 991.98px) {
        .cascading-right {
          margin-right: 0;
        }
      }
    </style>
  
    <!-- Jumbotron -->
    <div class="container py-4">
      <div class="row g-0 align-items-center">
        <div class="col-lg-6 mb-5 mb-lg-0">
          <div class="card cascading-right" style="
              background: hsla(0, 0%, 100%, 0.55);
              backdrop-filter: blur(30px);
              ">
            <div class="card-body p-5 shadow-5 text-center">
              <h2 class="fw-bold mb-5">MED MOVIL</h2>
              <form action="/logcli" method="post">
                <!-- 2 column grid layout with text inputs for the first and last names -->
                <div class="row">
                  <div class="col-md-6 mb-4">
                    <div class="form-outline">
                      <input type="text" name="Clinomb" class="form-control text-center" />
                      <label class="form-label" for="form3Example1">Nombre Completo</label>
                    </div>
                  </div>
                  <div class="col-md-6 mb-4">
                    <div class="form-outline">
                      <input type="text" name="Clinumb" class="form-control text-center" />
                      <label class="form-label" for="form3Example2">Celular</label>
                    </div>
                  </div>
                </div>
  
                <!-- Email input -->
                <div class="row  d-flex justify-content-center align-items-center h-100">
                <div class="form-outline col-md-7 mb-4">
                  <input type="email" name="username" class="form-control text-center" />
                  <label class="form-label" for="form3Example3">Email</label>
                </div>
  
                <!-- Password input -->
                <div class="form-outline col-md-7 mb-4">
                  <input type="password" name="pass" class="form-control text-center" />
                  <label class="form-label" for="form3Example4">Password</label>
                </div>
  
                </div>
                <!-- Submit button -->
                <button type="submit" class="btn btn-success btn-block mb-4">
                  Registrar
                </button>
            </form>
                <!-- Register buttons -->
                <div class="text-center">
                  <p>Conecta Con:</p>
                  <button type="button" class="btn btn-link btn-floating mx-1">
                    <i class="fab fa-facebook-f"></i>
                  </button>
  
                  <button type="button" class="btn btn-link btn-floating mx-1">
                    <i class="fab fa-google"></i>
                  </button>
  
                  <button type="button" class="btn btn-link btn-floating mx-1">
                    <i class="fab fa-twitter"></i>
                  </button>
  
                  <button type="button" class="btn btn-link btn-floating mx-1">
                    <i class="fab fa-github"></i>
                  </button>
                </div>
              
            </div>
          </div>
        </div>
  
        <div class="col-lg-6 mb-5 mb-lg-0">
          <img src="/assets/img/med.jpg" width="650" height="790" class="w-100 rounded-4 shadow-4" alt="" />
        </div>
      </div>
